# Paddle Billing Environment Variables
# Copy this file to .env and fill in your actual values

# ======================
# PADDLE CONFIGURATION
# ======================
# Your Paddle Billing API key
# Get from: https://developers.paddle.com
PADDLE_API_KEY=your_live_api_key_here

# Paddle product ID for TenantLease
PRODUCT_ID=pro_01kgrhkyabt3244vn6hqgj3ype

# Paddle price ID for $9.90 one-time payment
PRICE_ID=pri_01kgrhp2wrthebpgwmn8eh5ssy

# Paddle webhook secret for signature verification
# Get from: https://vendors.paddle.com/settings
PADDLE_WEBHOOK_SECRET=your_webhook_secret_here

# ======================
# SERVER CONFIGURATION
# ======================
# Port to run the server on
PORT=3001

# ======================
# OPTIONAL CONFIGURATION
# ======================
# Log level: DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# Enable debug mode (shows more detailed logs)
DEBUG=false
